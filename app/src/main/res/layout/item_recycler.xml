<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/wasteButton"
    android:layout_width="match_parent"
    android:layout_height="145dp">

    <TextView
        android:id="@+id/textShares"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        android:textColor="#070707"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textHash" />

    <TextView
        android:id="@+id/textViewHash1"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="50dp"
        android:text="AVG Hash :"
        android:textColor="#0B0A0A"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ethIcon"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textTem"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        android:textColor="#FF0000"
        app:layout_constraintBottom_toTopOf="@+id/textViewHash1"
        app:layout_constraintStart_toEndOf="@+id/ethIcon"
        app:layout_constraintTop_toBottomOf="@+id/textID"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ethIcon"
        android:layout_width="50dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/eth"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textID"
        android:layout_width="232dp"
        android:layout_height="36dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="4dp"
        android:text="TextView"
        android:textColor="#090909"
        android:textSize="24sp"
        android:textStyle="normal"
        app:layout_constraintStart_toEndOf="@+id/ethIcon"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/monitorCMD"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/monitor"
        tools:ignore="SpeakableTextPresentCheck" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"></View>

    <ImageButton
        android:id="@+id/goToCalc"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/monitorCMD"
        app:srcCompat="@drawable/go_to_front" />

    <TextView
        android:id="@+id/textHash"
        android:layout_width="100dp"
        android:layout_height="20dp"
        android:layout_marginBottom="50dp"
        android:text="TextView"
        android:textColor="#0E0ED6"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textViewHash1" />

    <TextView
        android:id="@+id/textUpTime"
        android:layout_width="120dp"
        android:layout_height="20dp"
        android:layout_marginStart="24dp"
        android:text="TextView"
        android:textColor="#121212"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ethIcon"
        app:layout_constraintTop_toBottomOf="@+id/textViewHash1" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/wastedelete" />

    <TextView
        android:id="@+id/textPower"
        android:layout_width="130dp"
        android:layout_height="20dp"
        android:text="TextView"
        android:textColor="#020202"
        app:layout_constraintBottom_toBottomOf="@+id/textTem"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.25"
        app:layout_constraintStart_toEndOf="@+id/textTem"
        app:layout_constraintTop_toTopOf="@+id/textTem" />


</androidx.constraintlayout.widget.ConstraintLayout>